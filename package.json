{
  "name": "au-pair",
  "version": "1.0.0",
  "description": "A digital au-pair that monitors the health of your dependents.",
  "keywords": "health, status, check, health check, status check, aupair",
  "repository": {
    "type": "git",
    "url": "git://github.com/bunk/au-pair.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore ./",
    "build": "babel -d lib/ src/ --ignore=\"*spec.js\"",
    "test": "npm run lint && npm run coverage",
    "test-and-exit": "NODE_ENV=test mocha --compilers js:babel-register -r ./spec-helpers -R spec 'src/**/*.spec.js'",
    "test-watch": "NODE_ENV=test mocha -w --compilers js:babel-register -r ./spec-helpers -R spec 'src/**/*.spec.js'",
    "coverage": "nyc -r text-summary -r html --require=babel-register -- npm run test-and-exit",
    "prepublish": "npm run build"
  },
  "author": "JD Courtoy <jd.courtoy@leankit.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "eslint": "^2.10.2",
    "eslint-config-leankit": "^2.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-uglify": "^1.5.3",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4",
    "run-sequence": "^1.2.1",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0"
  }
}
