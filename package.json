{
  "name": "aupair",
  "version": "1.1.1",
  "description": "A digital au-pair that monitors the health of your dependents.",
  "keywords": "health, status, check, health check, status check, aupair",
  "repository": {
    "type": "git",
    "url": "git://github.com/bunk/aupair.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "eslint --ignore-path .gitignore ./",
    "build": "npm run clean && babel -d lib/ src/ --ignore=\"*spec.js\" --source-maps",
    "test": "npm run lint && npm run coverage",
    "test-and-exit": "NODE_ENV=test mocha --compilers js:babel-register -r ./spec-helpers -R spec 'src/**/*.spec.js'",
    "test-watch": "NODE_ENV=test mocha -w --compilers js:babel-register -r ./spec-helpers -R spec 'src/**/*.spec.js'",
    "cover": "nyc -r text-summary -r html --require=babel-register -- npm run test-and-exit",
    "show-coverage": "open ./coverage/index.html",
    "pretest": "npm run lint",
    "preversion": "npm run cover",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "prepublish": "publish-please guard",
    "publish-please": "publish-please"
  },
  "author": "JD Courtoy <jd@courtoy.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-leankit": "^2.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4",
    "publish-please": "^2.3.1",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "joi": "^9.0.0-3",
    "moment-precise-range": "^0.1.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
